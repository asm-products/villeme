<h3><%= @city.name unless @city.nil? %></h3>
<div class="Grid Grid--withGutter">
  <%= render 'alert_none_events' %>

  <% if current_or_guest_user.guest? %>
      <%= render 'events_for_guest' %>
  <% else %>
      <%= render 'events', events: @events %>
  <% end %>

  <%= render 'alert_create_new_event', events: @events %>
</div>