
<div id="sidebar-right" class="ps-container">
  <div id="sidebar-map" class="panel panel-default affix">
  
    <div class="panel-body user-box sidebar no-padding">

      <div id="user-map" style="width:100%"></div>

    </div>

    <div id="neighborhood-count" class="panel-body">
      <ul class="list-group m-0 font-12">
        <%= render "newsfeed/sidebar/neighborhood_count" %>
      </ul>
    </div>

    <ul class="infos list-group">
      <li class="item list-group-item walk" title="Indo caminhando"><%= x_image_tag "walk.svg" %> <span class="data"></span> caminhando</li>
      <li class="item list-group-item bike" title="Indo de bicicleta"><%= x_image_tag "bike.svg" %> <span class="data"></span> pedalando</li>
      <li class="item list-group-item bus" title="Indo de ônibus"><%= x_image_tag "bus.svg" %> <span class="data"></span> de ônibus</li>
      <li class="item list-group-item car" title="Indo de carro"><%= x_image_tag "car.svg" %> <span class="data"></span> dirigindo</li>
    </ul>
    
  </div>





</div>
