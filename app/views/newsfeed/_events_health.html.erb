<section class="Section" data-anchor="health">
  <div class="Section-header">
    <h1 class="Section-title">Cuidar da saude</h1>
    <span class="Section-description"></span>
  </div>

  <div class="Grid Grid--withGutter">
    <%= render 'alert_none_events' %>

    <% if current_or_guest_user.guest? %>
        <%= render 'events_for_guest', events: @events_health %>
    <% else %>
        <%= render 'event', events: @events_health %>
    <% end %>

    <%= render 'alert_create_new_event', events: @events_health %>
  </div>
</section>

